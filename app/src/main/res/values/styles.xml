<resources>

    Base application theme.

    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">

    </style>

    <!--<style name="ImageTranslucentTheme" parent="AppTheme">-->
        <!--&lt;!&ndash;在Android 4.4之前的版本上运行，直接跟随系统主题&ndash;&gt;-->
    <!--</style>-->


    <style name="main_aty_tab">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
    </style>

    <style name="main_aty_tab_margin">
        <item name="android:layout_marginLeft">24dp</item>
        <item name="android:layout_marginRight">25dp</item>
    </style>

    <style name="main_aty_tab_relayout">

    </style>

    <style name="RatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_drawable</item>
        <item name="android:minHeight">20dip</item>
        <item name="android:maxHeight">20dip</item>
        <item name="android:minWidth">20dip</item>
        <item name="android:stepSize">0.5</item>
        <item name="android:isIndicator">true</item>
    </style>
    <style name="RatingBarOne" parent="@android:style/Widget.Holo.RatingBar.Small">
        <item name="android:minHeight">50dip</item>
        <item name="android:maxHeight">50dip</item>
        <item name="android:minWidth">50dip</item>
        <item name="android:stepSize">0.5</item>
        <item name="android:isIndicator">true</item>

    </style>


</resources>
